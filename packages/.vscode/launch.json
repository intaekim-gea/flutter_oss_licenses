    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_oss_licenses",
            "cwd": "flutter_oss_licenses",
            "request": "launch",
            "type": "dart",
            "program": "bin/generate.dart",
            "args": ["-o", "artifacts/oss_licenses.json", "--json"]
        },
        {
            "name": "generate markdown table",
            "cwd": "flutter_oss_licenses",
            "request": "launch",
            "type": "dart",
            "program": "bin/generate_md.dart",
            "args": ["-i", "artifacts/oss_licenses.json", "-o", "artifacts/oss_licenses.md", "-m", "artifacts/meta.json"]
        },
        {
            "name": "example",
            "cwd": "flutter_oss_licenses/example",
            "request": "launch",
            "type": "dart"
        },
    ]
}